{
	"user": "ZanderOperator",
	"repo": "open.RP",
	"entry": "gamemodes/main.pwn",
	"output": "gamemodes/main.amx",
	"dependencies": [
		"pawn-lang/samp-stdlib",
		"Zeex/samp-plugin-crashdetect:v4.19.4",
		"pBlueG/SA-MP-MySQL:R41-4",
		"Y-Less/sscanf:v2.8.3",
		"samp-incognito/samp-streamer-plugin:v2.9.5",
		"lassir/bcrypt-samp@master",
		"Southclaws/pawn-chrono",
		"Southclaws/progress2",
		"Southclaws/OnPlayerSlowUpdate",
		"Southclaws/samp-plugin-mapandreas",
		"MySekwel/New-SA-MP-callbacks",
		"kristoisberg/samp-map-zones",
    "oscar-broman/samp-weapon-config",
    "pawn-lang/YSI-Includes",
    "pawn-lang/sa-mp-fixes"
	],
	"local": true,
	"runtime":
  {
		"version": "0.3.DL",
		"mode": "server",
		"echo": "loading server.cfg generated by sampctl - do not edit this file manually, edit pawn.json instead!",
		"filterscripts": [
			"ls_elevator",
			"ls_beachside",
			"ServerMaps"			
		],
		"plugins": [
			"TPoker"
		],
		"rcon_password": "0000",
		"port": 7777,
		"hostname": "SA-MP Server",
		"maxplayers": 50,
		"language": "",
		"stream_distance": 300,
		"extra": {"useartwork": "1", "crashdetect_log": "errorlog.txt"}
	},
	"runtimes": [
		{
			"name": "dev",
			"rcon_password": "0000",
			"port": 7777,
			"hostname": "[open.RP] - OpenSourced RolePlay",
			"maxplayers": 10,
			"language": "English",
			"mapname": "Los Santos",
			"weburl": "open.mp",
			"announce": false,
			"lanmode": true,
			"query": false,
			"rcon": false,
			"logqueries": false,
			"maxnpc": 0,
			"stream_rate": 1000,
			"stream_distance": 300,
			"onfoot_rate": 80,
			"incar_rate": 80,
			"weapon_rate": 40,
			"logtimeformat": "[%d-%b-%Y %X]",
			"messageholelimit": 10000,
			"ackslimit": 3000,
			"lagcompmode": 1,
			"db_logging": false,
			"db_log_queries": false,
			"cookielogging": false
		}
	],
	"builds": [
		{
			"name": "dev",
			"args": [
				"-;+",
				"-(+",
				"-Z+",
				"-S32768",
				"-d3"
			],
			"includes": [
				"legacy/",
				"gamemodes/modules",
				"gamemodes/modules/Admin",
				"gamemodes/modules/Arrays",
				"gamemodes/modules/Char",
				"gamemodes/modules/Player",
				"gamemodes/modules/Preincludes",
				"gamemodes/modules/Server",
				"gamemodes/modules/Systems",
				"gamemodes/modules/time"
			],
			"compiler": {}
		},
		{
			"name": "live",
			"args": [
				"-;+",
				"-(+",
				"-Z+",
				"-O2",
				"-S8192",
				"-d3"
			],
			"includes": [
				"legacy/"
			],
			"constants": {
				"BUILD_MINIMAL": ""
			},
			"compiler": {}
		},
		{
			"name": "pre",
			"args": [
				"-;+",
				"-(+",
				"-Z+",
				"-S8192",
				"-l",
				"-d3"
			],
			"includes": [
				"legacy/"
			],
			"compiler": {}
		}
	]
}